{
  "scenarios": {
    "fspiopDiscovery": {
      "executor": "ramping-vus",
      "exec": "fspiopDiscoveryScenarios",
      "env": {
        "UNIDIRECTIONAL": "false"
      },
      "startVUs": 50,
      "stages": [
        { "duration": "2m", "target": 50 }
      ]
    }
  },
  "thresholds": {
    "iteration_duration": [ "p(95)<1000" ],
    "http_req_failed": [ "rate<0.01" ],
    "http_req_duration": [ "p(95)<1000" ]
  }
}
