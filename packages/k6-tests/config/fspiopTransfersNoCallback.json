{
  "noConnectionReuse": false,
  "scenarios": {
    "fspiopTransfers": {
      "executor": "shared-iterations",
      "exec": "fspiopTransfersNoCallbackScenarios",
      "env": {
        "UNIDIRECTIONAL": "false"
      },
      "vus": 20,
      "iterations": 50000
    }
  },
  "thresholds": {
    "iteration_duration": [ "p(95)<1000" ],
    "http_req_failed": [ "rate<0.01" ],
    "http_req_duration": [ "p(95)<1000" ]
  }
}
